<% if is_liked.present? %>
	<%= link_to  image_pack_tag('posts/heart_filled_red.svg', class: 'post-heart-icon'), like_path(is_liked), method: :delete, remote: true, data: {confirm: "Are you shure to cancel like?"} %>
<% else %>
	<%= link_to image_pack_tag('posts/heart.svg', class: 'post-heart-icon'), post_likes_path(post), class: "col-sm-1", method: :post, remote: true %>
<% end %>