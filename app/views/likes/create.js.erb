$('#like-icon-post-<%= @post.id.to_s %>').
  html("<%= link_to image_pack_tag('posts/heart_filled_red.svg', class: 'post-heart-icon'), like_path(@like), method: :delete, remote: true, data: {confirm: 'Are you shure to cancel like?'} %>");

$('#like-text-post-<%= @post.id.to_s %>').
  html("<%= j render 'posts/like_text', {likes: @post.likes} %>");
