$('#like-icon-post-<%= @post.id.to_s %>').
  html('<%= link_to image_pack_tag("posts/heart.svg", class: "post-heart-icon"), post_likes_path(@post), method: :post, remote: true %>');
$('#like-text-post-<%= @post.id.to_s %>').
  html('<%= j render "posts/like_text", {likes: @post.likes} %>');
